{
    "name": "spatie/laravel-responsecache",
    "description": "Speed up a Laravel application by caching the entire response",
    "keywords": [
        "spatie",
        "laravel-responsecache",
        "laravel",
        "cache",
        "response",
        "performance"
    ],
    "homepage": "https://github.com/spatie/laravel-responsecache",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/cache": "5.5.x-dev",
        "illuminate/config": "5.5.x-dev",
        "illuminate/console": "5.5.x-dev",
        "illuminate/contracts": "5.5.x-dev",
        "illuminate/http": "5.5.x-dev",
        "illuminate/support": "5.5.x-dev",
        "nesbot/carbon": "~1.19"
    },
    "require-dev": {
        "phpunit/phpunit" : "^6.2",
        "orchestra/testbench": "3.5.x-dev",
        "mockery/mockery": "^0.9.9"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\ResponseCache\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\ResponseCache\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\ResponseCache\\ResponseCacheServiceProvider"
            ],
            "aliases": {
                "ResponseCache": "Spatie\\ResponseCache\\ResponseCacheFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
