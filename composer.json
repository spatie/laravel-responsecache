{
    "name": "spatie/laravel-responsecache",
    "description": "Greatly speed up a Laravel application by caching the entire response",
    "keywords": [
        "spatie",
        "laravel-responsecache",
        "laravel",
        "cache",
        "response",
        "performance"
    ],
    "homepage": "https://github.com/spatie/laravel-responsecache",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.5.0",
        "illuminate/cache": "^5.1",
        "illuminate/config": "^5.1",
        "illuminate/console": "^5.1",
        "illuminate/http": "^5.1",
        "illuminate/support": "^5.1",
        "nesbot/carbon": "~1.19"
    },
    "require-dev": {
        "phpunit/phpunit" : "^4",
        "scrutinizer/ocular": "^1.1",
        "orchestra/testbench": "^3.0",
        "mockery/mockery": "^0.9.4"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\ResponseCache\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\ResponseCache\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
